{
	"$schema": "http://json.schemastore.org/eslintrc",
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["next/core-web-vitals", "prettier"],
	"plugins": ["prettier"],
	"parserOptions": {
		"parserOptions": {
			"ecmaVersion": 6,
			"sourceType": "module",
			"ecmaFeatures": {
				"modules": true
			}
		}
	},
	"rules": { "prettier/prettier": 2, "import-declaration ": "off" }
}
